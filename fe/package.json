{
    "name": "with-typescript",
    "version": "1.0.0",
    "scripts": {
        "dev": "next",
        "build": "next build",
        "start": "next start -p $PORT",
        "type-check": "tsc",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "codegen": "graphql-codegen --config codegen.yml",
        "format:generated": "prettier --write \"generated/**/*.ts\"",
        "schema": "apollo client:download-schema --endpoint=https://delta-helpdesk.herokuapp.com/graphql/ ./src/graphql/graphql-schema.json",
        "generate": "apollo client:codegen --localSchemaFile=./src/graphql/graphql-schema.json --target=typescript --includes=./src/graphql/**/*.ts --tagName=gql --addTypename --globalTypesFile=./src/graphql/graphql-global-types.ts types",
        "typescript": "tsc -p ./tsconfig.json --pretty --noEmit"
    },
    "dependencies": {
        "@material-ui/core": "^4.5.2",
        "@material-ui/icons": "^3.0.1",
        "@material-ui/lab": "^4.0.0-alpha.33",
        "@material-ui/styles": "^4.5.2",
        "@mdi/font": "^3.2.89",
        "@mdi/js": "^3.2.89",
        "@mdi/react": "^1.1.0",
        "@types/node-fetch": "^2.5.2",
        "@types/react-router-dom": "^5.1.0",
        "apollo": "2.21.0",
        "apollo-boost": "^0.4.4",
        "graphql": "^14.5.8",
        "moment": "^2.24.0",
        "msal": "^1.1.3",
        "next": "^9.1.4",
        "node-fetch": "^2.6.0",
        "react": "^16.10.1",
        "react-apollo": "^3.1.1",
        "react-dom": "^16.10.1",
        "react-localization": "^1.0.15",
        "react-router-dom": "^5.1.2",
        "react-snow-effect": "^1.2.0",
        "react-social-login": "^3.4.6",
        "react-trello-for-timeline": "^2.2.6",
        "tslint": "^5.20.0",
        "universal-cookie": "^4.0.2"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^1.8.3",
        "@graphql-codegen/typescript": "^1.8.3",
        "@graphql-codegen/typescript-operations": "1.3.1",
        "@graphql-codegen/typescript-react-apollo": "1.3.1",
        "@types/node": "^12.7.8",
        "@types/react": "^16.9.3",
        "@types/react-dom": "^16.9.1",
        "typescript": "3.7.2"
    },
    "license": "ISC"
}
