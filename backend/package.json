{
  "name": "backend",
  "version": "0.0.0",
  "description": "description",
  "license": "MIT",
  "scripts": {
    "build": "tsc && npm-run-all copy:locs",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts npm-run-all -s copy-mysql copy-sql copy-env",
    "start:remote": "cross-env-shell NODE_ENV=remote npm-run-all start",
    "start:local": "cross-env-shell NODE_ENV=local npm-run-all start",
    "start:dev": "nodemon -L",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "cross-env-shell NODE_ENV=prod node dist/src/main.js",
    "copy:locs": "ncp src/localization/localizations/ dist/src/localization/localizations/",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "copy-mysql": "ncp ../dckr/env/mysql-example.env ../dckr/env/mysql.env",
    "copy-sql": "ncp ../dckr/mysql/docker-entrypoint-initdb.d/createdb.sql.example ../dckr/mysql/docker-entrypoint-initdb.d/createdb.sql",
    "copy-env": "cpx \"/local.env-example\" \"/local.env\" -u",
    "begin": "ncp local.env-example local.env"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^2.0.0",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^6.5.1",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/typeorm": "^6.1.3",
    "@types/lodash": "^4.14.122",
    "apollo-server-express": "^2.2.2",
    "bcrypt": "^3.0.2",
    "cpx": "^1.5.0",
    "dotenv": "^8.1.0",
    "faker": "^4.1.0",
    "graphql": "^14.0.0",
    "graphql-tools": "^4.0.3",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "ncp": "^2.0.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.7",
    "typescript": "^3.6.3",
    "wait-on": "^3.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.7.2",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.4",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/passport": "^1.0.0",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.5",
    "cross-env": "6.0.0",
    "jest": "^24.9.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.2",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.2",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.20.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
