{
  "name": "backend",
  "version": "0.0.0",
  "description": "description",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:remote": "cross-env-shell NODE_ENV=remote npm-run-all start",
    "start:local": "cross-env-shell NODE_ENV=local npm-run-all start",
    "start:dev": "nodemon -L",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc && copyfiles --all 'src/localization/localizations' 'dist/src/localization/localizations'",
    "start:prod": "cross-env-shell NODE_ENV=prod node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "postinstall": "npm-run-all --serial prestart:prod a1 a2",
    "a1": "copyfiles  ../dckr/env/mysql-example.env ../dckr/env/mysql.env",
    "a2": "copyfiles  ../dckr/mysql/docker-entrypoint-initdb.d/createdb.sql.example ../dckr/mysql/docker-entrypoint-initdb.d/createdb.sql"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^1.3.0",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/graphql": "^5.4.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/typeorm": "^5.2.2",
    "@types/lodash": "^4.14.122",
    "apollo-server-express": "^2.2.2",
    "bcrypt": "^3.0.2",
    "dotenv": "^8.1.0",
    "faker": "^4.1.0",
    "graphql": "^14.0.0",
    "graphql-tools": "^4.0.3",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.7",
    "typescript": "^3.1.6",
    "wait-on": "^3.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.4",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/passport": "^1.0.0",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.5",
    "copyfiles": "^2.1.1",
    "cross-env": "5.2.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.11.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
