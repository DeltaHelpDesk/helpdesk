    "@mdi/js": "^3.2.89",
    "@mdi/react": "^1.1.0",
    "@types/classnames": "^2.2.6",
    "apollo-boost": "^0.1.16",
    "classnames": "^2.2.6",
    "env-cmd": "^10.0.1",
    "graphql": "^14.0.2",
    "i18next": "^15.0.7",
    "i18next-xhr-backend": "^2.0.1",
    "moment": "^2.24.0",
    "msal": "^0.2.4",
    "react": "^16.8.4",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.8.4",
    "react-i18next": "^10.5.2",
    "react-markdown": "^4.0.6",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0"
