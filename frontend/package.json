{
  "name": "frontend",
  "version": "0.1.0",
  "author": "Tomas Faltejsek <faltejsek9@gmail.com>",
  "private": true,
  "scripts": {
    "start": "yarn start:remote",
    "start:normal": "react-scripts-ts start",
    "start:local": "env-cmd -e local -r ./.env.json yarn start:normal",
    "start:remote": "env-cmd -e remote -r ./.env.json yarn start:normal",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "build-storybook",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.7.0",
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
